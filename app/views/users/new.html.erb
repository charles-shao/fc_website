<div class="container">
  <div class="col-md-6 col-md-offset-3">

    <div id="clearance" class="sign-up">
      <h2><%= t(".title") %></h2>

      <%= form_for @user do |form| %>
        <%= render partial: '/users/form', object: form %>

        <div class="form-actions">
          <%= form.submit class: 'btn btn-primary' %>
        </div>
      <% end %>
    </div>

  </div>
</div>
