<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2 class="pt-5">Members</h2>

      <table class="table">
        <thead class="thead-default">
            <th>Name</th>
        </thead>

        <tbody>
          <% Character.all.each do |character| %>
            <tr>
              <td>
                <%= character.name %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <%= link_to "Back", dashboard_index_path, class: 'btn btn-secondary' %>
    </div>
  </div>
</div>
